{
  "datasources": [
    {
      "name": "Prometheus",
      "type": "prometheus",
      "url": "http://prometheus:9090",
      "access": "proxy",
      "basicAuth": false,
      "jsonData": {
        "tlsSkipVerify": true
      }
    }
  ],
  "panels": [
    {
      "type": "graph",
      "title": "CPU Usage",
      "targets": [
        {
          "expr": "avg(rate(container_cpu_usage_seconds_total[5m])) by (instance)",
          "legendFormat": "{{instance}}",
          "refId": "A"
        }
      ],
      "xaxis": {
        "mode": "time"
      },
      "yaxes": [
        {
          "format": "percent",
          "label": "CPU Usage",
          "min": 0,
          "max": 1
        },
        {
          "format": "short"
        }
      ]
    },
    {
      "type": "graph",
      "title": "Memory Usage",
      "targets": [
        {
          "expr": "avg(container_memory_usage_bytes) by (instance)",
          "legendFormat": "{{instance}}",
          "refId": "B"
        }
      ],
      "xaxis": {
        "mode": "time"
      },
      "yaxes": [
        {
          "format": "bytes",
          "label": "Memory Usage",
          "min": 0
        },
        {
          "format": "short"
        }
      ]
    },
    {
      "type": "graph",
      "title": "Network Traffic",
      "targets": [
        {
          "expr": "sum(rate(container_network_receive_bytes_total[5m])) by (instance)",
          "legendFormat": "Received {{instance}}",
          "refId": "C"
        },
        {
          "expr": "sum(rate(container_network_transmit_bytes_total[5m])) by (instance)",
          "legendFormat": "Transmitted {{instance}}",
          "refId": "D"
        }
      ],
      "xaxis": {
        "mode": "time"
      },
      "yaxes": [
        {
          "format": "bytes",
          "label": "Network Traffic",
          "min": 0
        },
        {
          "format": "short"
        }
      ]
    }
  ],
  "schemaVersion": 26,
  "version": 1,
  "links": [],
  "refresh": "5s"
}